              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:self">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="maml.ProcessEndEvent" referenceName="processFlowElements"/>
              </firstModelOperations>
            </initialOperation>
          </fixes>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="validateMinimumOneIncomingEdge" level="WARNING" message="The element has no incoming edge and is thus not included in the process!" targetClass="maml.ProcessFlowElement">
          <audits auditExpression="aql:not self.previousElements->isEmpty() or (self.oclIsKindOf(maml::Event) and not self.oclIsKindOf(maml::ProcessEndEvent))"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="validateRoleAssignment" level="WARNING" message="The element has no role assigned!" targetClass="maml.InteractionProcessElement">
